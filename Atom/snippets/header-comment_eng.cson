      // ║                                                                                                           ║
      // ╚═══════════════════════════════════════════════════════════════════════════════════════════════════════════╝
    """,
    'description': '⚠️Create a fancy header comment (with border) [⚠️WARNING!!!! THIS DOESN\'T WORK AUTOMATICALY!!!]'
  },
  'JavaTypeHeaderComment': {
    'prefix' : 'java-type-header',
    'body' : """
      /*
      * Classe : ${1:class name}
      * Description : ${2:description}
