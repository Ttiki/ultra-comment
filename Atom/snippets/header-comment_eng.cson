#Java
'.source.java':
  'HeaderComment' : {
    'prefix' : 'header-comment',
    'body' : """
      // File Name : ${1:class name}
      // File description : ${2:description}
      // Author(s) : ${3:author(s)}
      // Date of creation : ${4:date}
      // License : ${5:license}
      //
      // Supplementary information : ${6:infos}
    """,
    'description' : 'Create a simple header comment'
  },
  'HeaderCommentTopBottomBorder' : {
    'prefix' : 'header-comment-top-bottom-border',
    'body' : """
      // ==================================================================================================
      // File Name : ${1:class name}
      // File description : ${2:description}
      // Author(s) : ${3:author(s)}
      // Date of creation : ${4:date}
      // License : ${5:license}
      // ==================================================================================================
      // Supplementary information : ${6:infos}
      // ==================================================================================================
    """,
    'description' : 'Create a header comment with a simple top and bottom border'
  },
  'FancyHeaderComment' : {
    'prefix' : 'fancy-header',
    'body' : """
      // ╔═══════════════════════════════════════════════════════════════════════════════════════════════════════════╗
      // ║    File Name : ${1:class name}                                                                            ║
      // ║    File description : ${2:description}                                                                    ║
